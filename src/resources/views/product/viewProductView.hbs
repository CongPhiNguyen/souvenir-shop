    btnAddReview.addEventListener('click', async () => {
        try {
            reviewStarInputError.textContent = '';
            if (reviewStarInputValue < 1 || reviewStarInputValue > 5) {
                console.log('invalid review star input');
                reviewStarInputError.textContent = 'Vui lòng đánh giá sản phẩm';
            }
            else {
                const productId = document.querySelector('#product-code').getAttribute('data-value');
                star = reviewStarInputValue;
                comment = reviewCommentInput.value;

                console.log(productId);
                console.log(star);
                console.log(comment);

                const res = await fetch('/product/add-review', {
                    method: 'POST',
                    body: JSON.stringify({ productId, star, comment }),
                    headers: {'Content-Type': 'application/json'}
                });
                postedReviewData = await res.json();
                if (postedReviewData) {
                    if (postedReviewData.review) {
                        console.log('post review successful');
                        console.log(postedReviewData.review);
                        showAlertMorriStore('alert-success', 'Gửi bài đánh giá thành công!');

                        const date = new Date(postedReviewData.review.createdAt);
                        var dateString = (date.getDate() < 10 ? ('0' + date.getDate().toString()) : date.getDate().toString()) + '/' + (date.getMonth() < 9 ? ('0' + (date.getMonth() + 1).toString()) : (date.getMonth() + 1).toString()) + '/' + date.getFullYear().toString() + ' ' + date.toTimeString().substring(0, 5);
                        var reviewHTML = `<li class="review-item">
                                                <div class="review-item-container row">
                                                    <div class="user-information col-4">
                                                        <img class="avatar" src="https://thumbs.dreamstime.com/b/default-avatar-profile-icon-vector-social-media-user-image-182145777.jpg"></img>
                                                        <div class="name">${postedReviewData.review.name}}</div>
                                                    </div>
                                                    <div class="review-container col-8">
                                                        <div class="rated-star">`;

                        for (i = 0; i < 5; i++) {
                            if (i < postedReviewData.review.star) {
                                reviewHTML += `<i class="product-rating-icon fas fa-star"></i>`;
                            }
                            else {
                                reviewHTML += `<i class="product-rating-icon far fa-star"></i>`;
                            }
                        }

                        reviewHTML += `</div>
                                        <div class="review-content my-2">
                                            ${postedReviewData.review.comment}
                                        </div>
                                        <div class="review-time">
                                            ${dateString}
                                        </div>
                                    </div>
                                </div>
                            </li>`;

                        reviewItemList.innerHTML += reviewHTML;

                        iconStarOnclick(0);
                        reviewCommentInput.value = '';
                    }
                    else if (postedReviewData.error) {
                        console.log('btn post review error');
                        console.log(postedReviewData.error);
                        showAlertMorriStore('alert-danger', 'Gửi đánh giá thất bại, vui lòng thử lại');
                    }
                    else {
                        console.log('btn post review error');
                        console.log(postedReviewData);
                        showAlertMorriStore('alert-danger', 'Gửi đánh giá thất bại, vui lòng thử lại');
                    }
                }
                else {
                    console.log('btn post review error');
                    showAlertMorriStore('alert-danger', 'Gửi đánh giá thất bại, vui lòng thử lại');
                }


            



            }
        }
        catch(err) {
            console.log(err);
        }


        




    });





</script>

